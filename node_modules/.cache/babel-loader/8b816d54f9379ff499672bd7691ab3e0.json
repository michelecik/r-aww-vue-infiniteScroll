{"ast":null,"code":"import \"core-js/modules/es.object.to-string.js\";\nimport { onBeforeMount, onMounted, ref } from \"vue\";\nimport RedditListContainer from \"./components/RedditListContainer.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    RedditListContainer: RedditListContainer\n  },\n  setup: function setup() {\n    var loading = ref();\n    var data = ref();\n    var after = ref(0);\n    var scrollHeight = ref();\n\n    var fetchData = function fetchData() {\n      loading.value = true;\n      fetch('https://www.reddit.com/r/aww.json?limit=5').then(function (res) {\n        return res.json();\n      }).then(function (body) {\n        loading.value = false;\n        data.value = body.data.children;\n      });\n    };\n\n    var fetchMoreData = function fetchMoreData() {\n      fetch('https://www.reddit.com/r/aww.json?limit=5&after=25').then(function (res) {\n        return res.json();\n      }).then(function (body) {\n        loading.value = false;\n        console.log(data.value); // data.value += body.data.children;\n\n        console.log('fetched');\n      });\n    };\n\n    window.addEventListener('scroll', function () {\n      var bottomOfWindow = document.documentElement.scrollTop + window.innerHeight === document.documentElement.offsetHeight;\n\n      if (bottomOfWindow) {\n        fetchMoreData();\n      }\n    });\n    fetchData();\n    return {\n      data: data,\n      loading: loading\n    };\n  }\n};","map":{"version":3,"mappings":";AAOA,SAASA,aAAT,EAAwBC,SAAxB,EAAmCC,GAAnC,QAA8C,KAA9C;AACA,OAAOC,mBAAP,MAAgC,sCAAhC;AAEA,eAAe;EACXC,IAAI,EAAE,KADK;EAEXC,UAAU,EAAE;IACRF,mBAAkB,EAAlBA;EADQ,CAFD;EAKXG,KALW,mBAKH;IACJ,IAAMC,OAAM,GAAIL,GAAG,EAAnB;IACA,IAAMM,IAAG,GAAIN,GAAG,EAAhB;IACA,IAAMO,KAAI,GAAIP,GAAG,CAAC,CAAD,CAAjB;IAEA,IAAMQ,YAAW,GAAIR,GAAG,EAAxB;;IAEA,IAAMS,SAAQ,GAAI,SAAZA,SAAY,GAAM;MACpBJ,OAAO,CAACK,KAAR,GAAgB,IAAhB;MACAC,KAAK,CAAC,2CAAD,CAAL,CACKC,IADL,CACU,UAACC,GAAD;QAAA,OAASA,GAAG,CAACC,IAAJ,EAAT;MAAA,CADV,EAEKF,IAFL,CAEU,UA